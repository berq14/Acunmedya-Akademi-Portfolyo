@model List<AcunmedyaAkademiPortfolio1.Models.TblService>

@{
    ViewBag.Title = "CreateService";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Servis oluştur</h2>

<form method="post">
    <label>Servis Başlığı</label>
    <input type="text" class="form-control" name="ServiceTitle" style="border:groove" />
    <br />
    <label>Servis Açıkalaması</label>
    <input type="text" class="form-control" name="Description" style="border:groove" />
    <br />  
    <label>Servis ikon</label>
    <input type="text" class="form-control" name="IconUrl" style="border:groove" />
    <br />
    <button class="btn btn-info">Tecrübe Ekle</button>
</form>