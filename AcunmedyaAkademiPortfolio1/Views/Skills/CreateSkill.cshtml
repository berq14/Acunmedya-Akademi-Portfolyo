@model List<AcunmedyaAkademiPortfolio1.Models.TblSkill>

@{
    ViewBag.Title = "CreateSkill";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Yetenek oluştur</h2>

<form method="post">
    <label>Yetenek Başlığı</label>
    <input type="text" class="form-control" name="SkillTitle" style="border:groove" />
    <br />
    <label>Yetenek Değeri</label>
    <input type="text" class="form-control" name="SkillValue" style="border:groove" />
    <br />
    <button class="btn btn-info">Tecrübe Ekle</button>
</form>